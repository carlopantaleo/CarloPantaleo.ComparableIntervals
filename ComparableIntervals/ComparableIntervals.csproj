    <PackageTags>intervals ranges comparable union intersection complement</PackageTags>
    <TargetFrameworks>net40;net45;net451;net452;net46;net461;net472;netstandard2.0</TargetFrameworks>
    <PackageVersion>2.0.0</PackageVersion>
